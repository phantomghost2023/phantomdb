{
  "dashboard": {
    "id": null,
    "title": "PhantomDB Performance",
    "tags": ["phantomdb", "performance", "database"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "Query Latency Distribution",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 9},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(phantomdb_query_duration_seconds_bucket[5m]))",
            "legendFormat": "Median",
            "format": "time_series"
          },
          {
            "expr": "histogram_quantile(0.90, rate(phantomdb_query_duration_seconds_bucket[5m]))",
            "legendFormat": "90th Percentile",
            "format": "time_series"
          },
          {
            "expr": "histogram_quantile(0.95, rate(phantomdb_query_duration_seconds_bucket[5m]))",
            "legendFormat": "95th Percentile",
            "format": "time_series"
          },
          {
            "expr": "histogram_quantile(0.99, rate(phantomdb_query_duration_seconds_bucket[5m]))",
            "legendFormat": "99th Percentile",
            "format": "time_series"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Query Duration"
          },
          {
            "format": "short"
          }
        ],
        "legend": {
          "show": true
        }
      },
      {
        "id": 2,
        "type": "heatmap",
        "title": "Query Duration Heatmap",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 9},
        "targets": [
          {
            "expr": "rate(phantomdb_query_duration_seconds_bucket[5m])",
            "format": "heatmap",
            "legendFormat": "{{le}}"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxis": {
          "format": "s",
          "label": "Query Duration"
        },
        "color": {
          "mode": "opacity"
        }
      },
      {
        "id": 3,
        "type": "graph",
        "title": "Throughput Metrics",
        "gridPos": {"x": 0, "y": 9, "w": 12, "h": 9},
        "targets": [
          {
            "expr": "rate(phantomdb_queries_total[5m])",
            "legendFormat": "Queries/sec",
            "format": "time_series"
          },
          {
            "expr": "rate(phantomdb_requests_total[5m])",
            "legendFormat": "Requests/sec",
            "format": "time_series"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Rate"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 4,
        "type": "graph",
        "title": "Connection Metrics",
        "gridPos": {"x": 12, "y": 9, "w": 12, "h": 9},
        "targets": [
          {
            "expr": "phantomdb_connections_active",
            "legendFormat": "Active Connections",
            "format": "time_series"
          },
          {
            "expr": "rate(phantomdb_connections_total[5m])",
            "legendFormat": "New Connections/sec",
            "format": "time_series"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Count"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 5,
        "type": "graph",
        "title": "Resource Utilization",
        "gridPos": {"x": 0, "y": 18, "w": 12, "h": 9},
        "targets": [
          {
            "expr": "phantomdb_storage_used_bytes / phantomdb_storage_total_bytes * 100",
            "legendFormat": "Storage Usage %",
            "format": "time_series"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "percent",
            "label": "Utilization"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 6,
        "type": "stat",
        "title": "Current Query Latency",
        "gridPos": {"x": 12, "y": 18, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(phantomdb_query_duration_seconds_bucket[5m]))",
            "format": "time_series"
          }
        ],
        "options": {
          "fieldOptions": {
            "calcs": ["last"]
          },
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto",
          "orientation": "horizontal"
        },
        "valueFontSize": "80%"
      },
      {
        "id": 7,
        "type": "stat",
        "title": "Current Throughput",
        "gridPos": {"x": 18, "y": 18, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(phantomdb_queries_total[5m])",
            "format": "time_series"
          }
        ],
        "options": {
          "fieldOptions": {
            "calcs": ["last"]
          },
          "colorMode": "value",
          "graphMode": "none"
        }
      },
      {
        "id": 8,
        "type": "graph",
        "title": "Error Rates",
        "gridPos": {"x": 12, "y": 22, "w": 12, "h": 7},
        "targets": [
          {
            "expr": "rate(phantomdb_errors_total[5m])",
            "legendFormat": "Errors/sec",
            "format": "time_series"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Rate"
          },
          {
            "format": "short"
          }
        ]
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ]
    }
  }
}